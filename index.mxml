<?xml version="1.0" encoding="utf-8"?>
<mx:Application 
	xmlns:fx="http://ns.adobe.com/mxml/2009" 
	xmlns:mx="library://ns.adobe.com/flex/mx" 
	xmlns:local="*"
	layout="vertical" 
	verticalAlign="middle"
>
	
	<fx:Script>
		<![CDATA[
			import mx.collections.ArrayCollection;
			[Bindable] private var currencyList:ArrayCollection = new ArrayCollection( [ "AUD", "CNY", "EUR", "GBP", "USD" ] );
		]]>
	</fx:Script>
	
	<local:CurrencyComboBox width="100" dataProvider="{currencyList}" />
	
</mx:Application>
